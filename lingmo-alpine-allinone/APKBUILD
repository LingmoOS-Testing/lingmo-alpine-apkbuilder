build() {
    local apk
    for apk in $apks; do
	echo "Compiling $apk"
    	mkdir -pv "$srcdir/$apk/build" && cd "$srcdir/$apk/build"
        cmake -DCMAKE_INSTALL_PREFIX=/usr -DCMAKE_INSTALL_LIBDIR=/usr/lib ..
    	make -j8 || return 1
    done
}
package() {
    mkdir -pv "$pkgdir"
    local apk
    for apk in $apks; do
        cd "$srcdir/$apk/build"
        make -j8 DESTDIR="$pkgdir" install
    done
}
